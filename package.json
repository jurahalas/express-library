{
  "name": "express-locallibrary",
  "version": "0.0.0",
  "engines": {
    "node": "10.11.0"
  },
  "private": true,
  "scripts": {
    "start-dev": "babel-node ./src/server.js",
    "seed": "nodemon --exec babel-node populatedb.js",

      "clean": "rm -rf build && mkdir build",
      "build-babel": "babel -d ./build ./src -s",

